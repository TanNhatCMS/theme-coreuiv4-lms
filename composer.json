{
    "name": "tannhatcms/theme-coreuiv4-lms",
    "description": "UI for Backpack v6 that uses CoreUI v4 and Bootstrap v5.",
    "license": "MIT",
    "authors": [
        {
            "name": "TanNhatCMS",
            "email": "mrkiet.dev@gmail.com",
            "homepage": "https://mrkiet.name.vn"
        }
    ],
    "homepage": "https://github.com/TanNhatCMS/theme-coreuiv4-lms",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "ThemeCoreuiv4", "Theme LMS CMS"
    ],
    "require": {
        "tannhatcms/crud-lms": "^6.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "10.1.0"
    },
    "autoload": {
        "psr-4": {
            "TanNhatCMS\\ThemeCoreuiv4LMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TanNhatCMS\\ThemeCoreuiv4LMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TanNhatCMS\\ThemeCoreuiv4LMS\\AddonServiceProvider"
            ]
        }
    }
}
